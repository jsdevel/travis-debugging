{
  "name": "webdriver-sync",
  "version": "0.9.2",
  "description": "Synchronous testing with Selenium and node.js!",
  "main": "src/webdriver-sync.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "lint-src": "node_modules/.bin/jshint --config .jshintrc src/**/*.js",
    "lint-test": "node_modules/.bin/jshint --config .test-jshintrc test/**/*.js",
    "mocha": "node_modules/.bin/mocha -r should -t 8000 test/*.js test/classes/*.js test/enums/*.js test/interfaces/*.js",
    "test": "npm run lint-src && npm run lint-test && npm run mocha",
    "install": "node bin/postInstall.js",
    "prepublish": "bin/prepublish.bash"
  },
  "dependencies": {
    "async": "~0.2.9",
    "download": "~0.1.7",
    "java": "0.3.3",
    "unzip": "~0.1.9",
    "which": "~1.0.5"
  },
  "devDependencies": {
    "jshint": "",
    "mocha": "",
    "proxyquire": "",
    "sinon": "",
    "should": "~3.1.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jsdevel/webdriver-sync.git"
  },
  "keywords": [
    "webdriver",
    "selenium",
    "testing",
    "sync",
    "synchronous",
    "api",
    "chrome",
    "safari",
    "internet explorer",
    "explorer",
    "internet",
    "ie",
    "driver",
    "node",
    "javascript"
  ],
  "author": "Joseph Spencer",
  "license": "MIT license (MIT)",
  "bugs": {
    "url": "https://github.com/jsdevel/webdriver-sync/issues"
  }
}
